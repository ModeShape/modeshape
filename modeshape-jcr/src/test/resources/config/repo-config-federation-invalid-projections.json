{
    "name" : "Federated repository",
    "workspaces" : {
        "predefined" : ["ws1", "ws2"],
        "default" : "default",
        "allowCreation" : true
    },
    "externalSources" : {
        "mock-source" : {
            "classname" : "org.modeshape.connector.mock.MockConnector",
            "cacheTtlSeconds" : 1,
            "projections" : [
                "ws1:a/b/c => /d",
                ":/a/b => /",
                "ws1/ => /a/d",
                "ws2:/a//v => /",
                "ws => /a",
                ":x => a",
                "ws1:/ x",
                "ws1:/a/b =>"
            ]
        }
    }
}