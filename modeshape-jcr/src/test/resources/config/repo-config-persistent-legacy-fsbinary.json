{
    "name": "Persistent Repository With Legacy FS Binary Store",
    "storage": {
        "cacheName": "persistentRepository",
        "cacheConfiguration": "config/infinispan-persistent.xml",
        "binaryStorage": {
            "type": "file",
            "directory": "target/legacy_fs_binarystore"
        }
    },
    "workspaces": {
        "default": "default",
        "allowCreation": true
    }
}