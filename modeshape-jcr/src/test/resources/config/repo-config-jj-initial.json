{
    "name": "JJ initial repository",
    "storage": {
        "persistence" : {
            "type" : "db",
            "connectionUrl": "jdbc:h2:file:./target/persistent_repository/db"
        } ,
        "binaryStorage": {
            "type": "file",
            "directory": "target/persistent_repository/binaries",
            "minimumBinarySizeInBytes": 40
        }
    },
    "workspaces": {
        "default": "default",
        "allowCreation": true
    },
    "node-types": ["cnd/jj_initial.cnd"]
}