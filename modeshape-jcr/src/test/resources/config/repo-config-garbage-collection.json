{
    "name" : "Garbage collecting repository",
    "workspaces" : {
        "default" : "default",
        "allowCreation" : true
    },
    "security" : {
        "anonymous" : {
            "roles" : ["readonly","readwrite","admin"],
            "useOnFailedLogin" : true
        },
    },
    "garbageCollection" : {
        "initialTime" : "05:00",
        "intervalInHours" : 5,
        "lockCleanup" : {
            "lockCleanupInitialDelayInMinutes" : 0,
            "lockCleanupIntervalInMinutes" : 1
        }
    }
}