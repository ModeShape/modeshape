{
    "name" : "Federated repository",
    "workspaces" : {
        "predefined" : ["ws1", "ws2"],
        "default" : "default",
        "allowCreation" : true
    },
    "externalSources" : {
        "file-resources" : {
            "classname" : "org.modeshape.jcr.federation.FileSystemConnector",
            "directoryPath" : "target/federation/files",
        },
        "mock-source" : {
            "classname" : "org.modeshape.jcr.federation.MockConnector",
            "cacheTtlSeconds" : 1
        }
    }
}