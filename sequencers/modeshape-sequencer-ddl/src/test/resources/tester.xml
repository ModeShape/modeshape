<lexer>
    <state name="delegate">
        <char expression="success" destination="success" action="success"/>
        <!-- delegate everything -->
        <char expression=".+" destination="delegate" action="delegate"/>
    </state>
    
    <state name="success">
        <char expression="eos" destination="success" action="success"/>
    </state>
</lexer>
