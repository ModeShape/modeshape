{
    "name" : "Persisted-Repository",
    "workspaces" : {
        "predefined" : ["otherWorkspace"],
        "default" : "default",
        "allowCreation" : true
    },
    "security" : {
        "anonymous" : {
            "roles" : ["readonly","readwrite","admin"],
            "useOnFailedLogin" : false
        }
    },
    "externalSources" : {
        "cmis" : {
            "classname" : "org.modeshape.connector.CmisConnector",
            "cacheTtlSeconds" : 5,
            "aclService" : "http://localhost:8080/modeshape-cmis/services/ACLService?wsdl",
            "projections" : [ "default:/cmis => /" ]
        }
    },
    "storage" : {
        "cacheName" : "persistentRepository",
        "cacheConfiguration" : "infinispan-federation-persistent.xml"
    }
}