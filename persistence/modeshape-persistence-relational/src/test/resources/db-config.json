{
    // Used for docker integration testing where the values for the properties are set as system properties
    "type": "db",
    "connectionUrl": "${db.url}",
    "poolSize" : 5,
    "minIdlepoolSize" : 5,
    "driver": "${db.driver}",
    "username": "${db.username}",
    "password": "${db.password}",
    "createOnStart": true,
    "dropOnExit": true
}

